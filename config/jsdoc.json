{
	"tags": {
		"allowUnknownTags": false
	},
	"source": {
		"include": "./",
		"includePattern": "\\.js$",
		"excludePattern": "(node_modules|docs|test|coverage)"
	},
	"plugins": [
		"plugins/markdown"
	],
	"opts": {
		"template": "node_modules/docdash",
		"encoding": "utf8",
		"destination": "../tonylukasavage.github.io",
		"recurse": true,
		"verbose": true,
		"readme": "./README.md"
	},
	"docdash": {
		"static": true,
		"collapse": true,
		"menu": {
			"Github": {
				"href": "https://github.com/tonylukasavage/lucid-dream",
				"target": "_blank",
				"class": "menu-item",
				"id": "github-link"
			},
			"Discord": {
				"href": "https://discord.gg/BHJ964k",
				"target": "_blank",
				"class": "menu-item",
				"id": "github-link"
			}
		}
	}
}